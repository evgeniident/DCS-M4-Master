<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
<!-- Include your PyVCP panel here.The contents of this file will not be overwritten when you run stepconf again.-->

	
<hbox>
	<labelframe text="X">
	<vbox>

		<meter>
			<halpin>"vel_x"</halpin>
			<text>"VELOCITY"</text>
			<subtext>"X"</subtext>
			<size>130</size>
			<min_>-35</min_>
			<max_>35</max_>
			<majorscale>10</majorscale>
			<minorscale>5</minorscale>
			<region1>(-25,-35,"red")</region1>
			<region2>(25,35,"red")</region2>
		</meter>
		<hbox>
				<led halpin="limit_x_licht" on_color="yellow" off_color="black" />
				
				<label>
					<text>" Limit"</text>
					<font>("Helvetica",14)</font>
			
				</label>

		</hbox>
		<hbox>
				<led halpin="home_x_licht" on_color="yellow" off_color="black" />
				
				<label>
					<text>" Home"</text>
					<font>("Helvetica",14)</font>
			
				</label>
		</hbox>	
	</vbox>
	</labelframe>


	<labelframe text="Y">
	<vbox>

		<meter>
			<halpin>"vel_y"</halpin>
			<text>"VELOCITY"</text>
			<subtext>"Y"</subtext>
			<size>130</size>
			<min_>-35</min_>
			<max_>35</max_>
			<majorscale>10</majorscale>
			<minorscale>5</minorscale>
			<region1>(-25,-35,"red")</region1>
			<region2>(25,35,"red")</region2>
		</meter>
		<hbox>
				<led halpin="limit_y_licht" on_color="yellow" off_color="black" />
				
				<label>
					<text>" Limit"</text>
					<font>("Helvetica",14)</font>
			
				</label>
				
		</hbox>
		<hbox>
				<led halpin="home_y_licht" on_color="yellow" off_color="black" />
				
				<label>
					<text>" Home"</text>
					<font>("Helvetica",14)</font>
			
				</label>
		</hbox>	

	</vbox>
	</labelframe>

	<labelframe text="Z">
	<vbox>

		<meter>
			<halpin>"vel_z"</halpin>
			<text>"VELOCITY"</text>
			<subtext>"z"</subtext>
			<size>130</size>
			<min_>-20</min_>
			<max_>20</max_>
			<majorscale>10</majorscale>
			<minorscale>5</minorscale>
			<region1>(-15,-20,"red")</region1>
			<region2>(15,20,"red")</region2>
		</meter>
		<hbox>
				<led halpin="limit_z_licht" on_color="yellow" off_color="black" />
				
				<label>
					<text>" Limit"</text>
					<font>("Helvetica",14)</font>
			
				</label>
		</hbox>
		<hbox>
				<led halpin="home_z_licht" on_color="yellow" off_color="black" />
				
				<label>
					<text>" Home"</text>
					<font>("Helvetica",14)</font>
			
				</label>
		</hbox>	

	</vbox>
	</labelframe>
</hbox>






