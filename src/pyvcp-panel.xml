<vbox> <!-- Spindle-Speed -->
    <spinbox>
    <halpin>"spindle-speed"</halpin>
    <min_>100</min_>
    <max_>600</max_>
    <initval>0</initval>
    <resolution>10</resolution>
    <format>"2.3f"</format>
    <font>("Arial",30)</font>
    <param_pin>1</param_pin>
</spinbox>
</vbox>

