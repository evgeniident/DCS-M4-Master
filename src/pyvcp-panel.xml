	</labelframe>

	<labelframe text="Jog Speed">
			

<!-- jogging -->
	

<vbox> 
	<dial>
		<size>150</size>
		<cpr>100</cpr>
		<min_>0</min_>
		<max_>1500</max_>
		<text>"Dial"</text>
		<initval>0</initval>
		<resolution>1.0</resolution>
		
		<halpin>"speed"</halpin>

		<dialcolor>"yellow"</dialcolor>
		<edgecolor>"green"</edgecolor>
		<dotcolor>"black"</dotcolor>
		<param_pin>1</param_pin>
	</dial>
</vbox>
</labelframe>
</hbox>



<labelframe text="Funktions Button">


<hbox> 
	<vbox> 
		<button>
			<halpin>"set-tool-length"</halpin>
